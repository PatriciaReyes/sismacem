function verificarConsultaPlan(form){

 

     if (form.costo_estimado.value!="" && isNaN(form.costo_estimado.value)){
        alert("Introduzca un valor Valido para el Costo Estimado del Plan!");
        form.costo_estimado.focus();
        return false;
    }

     if (form.costo_total.value!="" && isNaN(form.costo_total.value)){
        alert("Introduzca un valor Valido para el Costo Total del Plan!");
        form.costo_total.focus();
        return false;
    }
     

    
        form.submit();
        
   

}
